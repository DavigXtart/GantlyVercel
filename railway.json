{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "psicoapp/Dockerfile",
    "buildContext": "psicoapp"
  },
  "deploy": {
    "startCommand": "java $JAVA_OPTS -jar app.jar",
    "restartPolicyType": "ON_FAILURE",
    "restartPolicyMaxRetries": 10
  }
}

